<configuration>

    <connectionStrings>
        <add name="programaholics_anonymous_databaseConnectionString"
            connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=|DataDirectory|\programaholics_anonymous_database.accdb"
            providerName="System.Data.OleDb" />
    </connectionStrings>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
      <pages enableViewState="true" />
    </system.web>

</configuration>
